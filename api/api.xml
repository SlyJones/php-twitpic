<api>

	<!-- Media Resources -->
	<category name="media">
		<endpoint name="show" method="GET" auth_required="false">
			<formats>xml,json</formats>
			<param name="id" type="integer" required="true"/>
			<param name="id2" type="integer" required="false"/>
		</endpoint>
	</category>

</api>